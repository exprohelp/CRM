
@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<script src="~/Areas/Masters/JsModule/DoctorRegistraion.js?his-version-1.0"></script>
<style>
    .panel {
        margin: 5px 3px 0 5px !important;
        border: 1px solid #bedbf6;
    }

    .panel-body {
        border: 1px solid #e1dede;
        padding: 5px 3px 3px 3px !important;
    }

    .form-group {
        margin-bottom: 5px;
    }

        .form-group label {
            padding: 3px 0 4px 8px !important;
        }

    .form-control {
        height: 24px;
        padding: 4px 2px;
    }

    textarea, .textarea, input, .input, select, .select {
        border: 1px solid #000 !important;
    }

    .selected {
        background: DodgerBlue !important;
        color: #fff;
    }

    highlight_row {
        background-color: #d5e7f8;
        color: #000;
    }

    .highlight {
        background-color: red;
    }

    .text-white {
        color: #fff;
    }

    .nav > li > a {
        padding: 5px 5px;
    }

    .nav > li.active > a {
        color: #000;
        background: #fff;
    }

    .nav-tabs {
        padding: 0;
        border: 0;
    }
</style>

<div id="page-content">
    <div style="height:560px; overflow-y:auto">
        <div class="col-lg-3">
            <div class="panel">
                <div class="panel-heading" style="background:#0076d0;padding: 4px;">
                    <h4 class="text-white">Doctors List</h4>
                </div>
                <div class="panel-body" style="background:#fff;">
                    <div class="form-group">
                        <div class="radio-inline">
                            <input type="radio" name="optradio" value="1" checked>Active
                        </div>
                        <div class="radio-inline">
                            <input type="radio" name="optradio" value="0">Deactive
                        </div>
                        <button type="button" class="btn btn-success pull-right" onclick="return BindDoctorList();">Load</button>
                    </div>
                    <div class="col-lg-12">
                        <div class="form-group">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="tblDoctorsList">
                                    <thead>
                                        <tr>
                                            <th>Code</th>
                                            <th style="width:30%">Name</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-5">
            <div class="panel">
                <div class="panel-heading" style="background:#0076d0;padding: 4px;">
                    <input type="button" onclick="return Clear()" class="btn btn-3d" value="New">
                    <h4 class="text-white pull-right"><span id="docname"></span><span id="docid"></span></h4>
                </div>

                <div class="panel-body" style="background:#fff;">
                    <div class="form-group row">
                        <label class="col-sm-2">Title</label>
                        <div class="col-sm-4">
                            <select id="ddlTitle" class="form-control">
                                <option value="0">Please Select</option>
                                <option value="Mr.">Mr.</option>
                                <option value="Mrs.">Mrs.</option>
                                <option value="Miss.">Miss.</option>
                                <option value="Master.">Master.</option>
                                <option value="Baby.">Baby.</option>
                                <option value="B/O.">B/O.</option>
                                <option value="Dr.">Dr.</option>
                                <option value="Prof.">Prof.</option>
                                <option value="Madam.">Madam.</option>
                                <option value="Selvi.">Selvi.</option>
                                <option value="Selvan.">Selvan.</option>
                                <option value="Sister.">Sister.</option>
                                <option value="Mohd.">Mohd.</option>
                            </select>
                        </div>

                        <label class="col-sm-2">Name</label>
                        <div class="col-sm-4">
                            <input type="text" id="txtDoctorName" class="form-control" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Dr. Type</label>
                        <div class="col-sm-4">
                            <select id="ddlDoctorType" class="form-control">
                                <option value="0">Please Select</option>
                                <option value="CONSULTANT">CONSULTANT</option>
                                <option value="ANESTHETIC">ANESTHETIC</option>
                            </select>
                        </div>

                        <label class="col-sm-2">Phone</label>
                        <div class="col-sm-4">
                            <input type="text" id="txtPhone" data-count="12" data-int class="form-control" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Mobile</label>
                        <div class="col-sm-4">
                            <input type="text" id="txtMobile" data-count="10" data-int class="form-control" />
                        </div>

                        <label class="col-sm-2">Address</label>
                        <div class="col-sm-4">
                            <textarea id="txtAddress1" name="txtAddress1" rows="1" cols="6" class="form-control"></textarea>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Specialization</label>
                        <div class="col-sm-4">
                            <div class="input-group" style="display:flex">
                                <select id="ddlSpecialization" class="form-control"></select>
                                <div class="input-group-append">
                                    <i class="fa fa-plus" data-toggle="modal" data-target="#SpecModal" style="margin:2px 0 0 2px;background:#0076d0;padding: 5px;color: #fff;"></i>
                                </div>
                            </div>
                        </div>


                        <label class="col-sm-2">Department</label>
                        <div class="col-sm-4">
                            <div class="input-group" style="display:flex">
                                <select id="ddlDepartment" class="form-control"></select>
                                <div class="input-group-append">
                                    <i class="fa fa-plus" data-toggle="modal" data-target="#DepartmentModal" style="margin:2px 0 0 2px;background:#0076d0;padding: 5px;color: #fff;"></i>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Degree</label>
                        <div class="col-sm-4">
                            <div class="input-group" style="display:flex">
                                <select id="ddlDegree" class="form-control"></select>
                                <div class="input-group-append">
                                    <i class="fa fa-plus" data-toggle="modal" data-target="#DegreeModal" style="margin:2px 0 0 2px;background:#0076d0;padding: 5px;color: #fff;"></i>
                                </div>
                            </div>
                        </div>

                        <label class="col-sm-2">Gender</label>
                        <div class="col-sm-4">
                            <select id="ddlgender" class="form-control">
                                <option value="0">Please Select</option>
                                <option value="Male">Male</option>
                                <option value="Female">Female</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">IMA Reg. No.</label>
                        <div class="col-sm-4">
                            <input type="text" id="txtRegImaNo" data-count="25" class="form-control" />
                        </div>

                        <label class="col-sm-2">Reg. Date</label>
                        <div class="col-sm-4">
                            <input type="date" id="txtRegDate" class="form-control" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Floor No</label>
                        <div class="col-sm-4">
                            <select id="ddlFloorNo" class="form-control">
                                <option value="">Please Select</option>
                                <option value="Basement">Basement</option>
                                <option value="Ground">Ground</option>
                                <option value="First Floor">First Floor</option>
                                <option value="Second Floor">Second Floor</option>
                            </select>
                        </div>

                        <label class="col-sm-2">Room No</label>
                        <div class="col-sm-4">
                            <select id="ddlRoomNo" class="form-control">
                                <option value="">Please Select</option>
                                <option value="B1">B1</option>
                                <option value="B2">B2</option>
                                <option value="G1">G1</option>
                                <option value="G2">G2</option>
                                <option value="F1">F1</option>
                                <option value="F2">F2</option>
                                <option value="S1">S1</option>
                                <option value="S2">S2</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Fee Freq.</label>
                        <div class="col-sm-4">
                            <input type="text" id="txtFeeFreq" data-int class="form-control" />
                        </div>

                        <label class="col-sm-2">Duration</label>
                        <div class="col-sm-4">
                            <select id="ddlDuration" class="form-control">
                                <option value="">Please Select</option>
                                <option value="5">5</option>
                                <option value="10" selected>10</option>.
                                <option value="15">15</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Emer. Avail</label>
                        <div class="col-sm-4">
                            <label><input type="radio" value="Y" name="Emrgavail"> Yes</label>
                            <label><input type="radio" value="N" name="Emrgavail" checked> No</label>
                        </div>

                        <label class="col-sm-3">Doctor Share</label>
                        <div class="col-sm-3">
                            <label><input type="radio" value="Y" name="docshare"> Yes</label>
                            <label><input type="radio" value="N" name="docshare" checked> No</label>
                        </div>


                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Online Appt</label>
                        <div class="col-sm-4">
                            <label><input type="radio" value="Y" name="onlineAppoint" checked> Yes</label>
                            <label><input type="radio" value="N" name="onlineAppoint"> No</label>
                        </div>

                        <label class="col-sm-3">Is Token Req.</label>
                        <div class="col-sm-3">
                            <label><input type="radio" name="Token" value="1" checked> Yes</label>
                            <label><input type="radio" name="Token" value="0"> No</label>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2">Status</label>
                        <div class="col-sm-4">
                            <label><input type="radio" value="1" name="drstatus" checked> Active</label>
                            <label><input type="radio" value="0" name="drstatus"> Deactive</label>
                        </div>
                    </div>

                    <input type="button" id="btnSaveDoctor" class="btn btn-primary pull-right" value="Save" onclick="return AddUpdateDoctorProfile();" />
                </div>
            </div>

        </div>

        <div class="col-md-4">
            <div class="panel">
                <div class="panel-heading" style="background:#0076d0;padding: 4px; display:flex">
                    <div class="col-lg-5">
                        <ul class="nav nav-tabs">
                            <li class="active"><a href="#tab1primary" data-toggle="tab">By Days</a></li>
                            <li><a href="#tab2primary" data-toggle="tab">By Date</a></li>
                        </ul>
                    </div>

                    <div class="col-lg-4">
                        <h5 class="text-white">OPD Schedule</h5>
                    </div>

                    <div class="col-lg-3">
                        <span style="float:right" class="text-white" id="EditDocId"></span>
                    </div>


                </div>

                <div class="panel-body">
                    <div class="tab-content">
                        <div class="tab-pane fade in active" id="tab1primary">
                            <div class="col-lg-12">
                                <div class="form-group" style="font-weight:bold">
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Monday">Mon
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Tuesday">Tue
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Wednesday">Wed
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Thursday">Thu
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Friday">Fri
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Saturday">Sat
                                    </label>
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="daysname" value="Sunday">Sun
                                    </label>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-2">Shift</label>
                                <div class="col-sm-4">
                                    <select id="ddlShift" class="form-control">
                                        <option value="">Please Select </option>
                                        <option value="Morning">Morning</option>
                                        <option value="Evening">Evening</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-2">Start Time</label>
                                <div class="col-sm-2">
                                    <input type="number" id="txtStartTime" class="form-control" min="01" max="12" />
                                </div>
                                <div class="col-sm-2">
                                    <select id="ddlStartSLots" class="form-control">
                                        <option value="AM">AM</option>
                                        <option value="PM">PM</option>
                                    </select>
                                </div>

                                <label class="col-sm-2">End Time</label>
                                <div class="col-sm-2">
                                    <input type="number" id="txtEndTime" class="form-control" min="01" max="12" />
                                </div>
                                <div class="col-sm-2">
                                    <select id="ddlEndSLots" class="form-control">
                                        <option value="AM">AM</option>
                                        <option value="PM">PM</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-2">Pat. Limit</label>
                                <div class="col-sm-4">
                                    <input type="text" id="txtpatientLimit" data-int class="form-control" minlength="1" maxlength="2" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <input type="button" id="btnAddSlot" class="btn btn-primary pull-right" value="Add" onclick="return AddUpdateDoctorSlot();" />
                            </div>                            

                            <div class="form-group row">
                                <div class="panel-heading" style="background:#0076d0;padding: 4px;">
                                    <h4 class="text-white">Slot Time</h4>
                                </div>

                                <div class="table-responsive">
                                    <table class="table table-bordered" id="tblopdSchedule">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>Days</th>
                                                <th>St. Time</th>
                                                <th>Ed. Time</th>
                                                <th>Patient Limit</th>
                                                <th>Shift</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>



                        </div>

                        <div class="tab-pane fade" id="tab2primary">
                            <div class="form-group row" style="display:flex">
                                <label class="col-sm-2">From</label>
                                <div class="col-sm-4">
                                    <input type="date" id="txtFromDate" class="form-control" />
                                </div>

                                <label class="col-sm-2">To</label>
                                <div class="col-sm-4">
                                    <input type="date" id="txttoDate" class="form-control" />
                                </div>
                                <span style="margin: 2px 0 0 2px;background: #0076d0;padding: 3px 9px 0 7px;color: #fff;height: 22px;" onclick="return GenerateRow();">
                                    <i class="fa fa-arrow-left"></i>
                                </span>
                            </div>

                            <div class="form-group">
                                <div class="table-responsive">
                                    <table class="table table-bordered" id="tbldynamic">                                        
                                    </table>
                                </div>
                            </div>

                            <div class="form-group row">
                                <input type="button" id="btnAddSlot" class="btn btn-primary pull-right" value="Save" />
                            </div>


                        </div>

                    </div>
                </div>



                
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="SpecModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Add Specialization</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>Specialization</label>
                    <input type="text" class="form-control" id="txtSpec">
                </div>
                <div class="form-group">
                    <label>Description</label>
                    <textarea type="text" class="form-control" id="txtSpecDesc"></textarea>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="return AddSpecialization();">Save</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="DepartmentModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Add Department</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>Department</label>
                    <input type="text" class="form-control" id="txtDepartment">
                </div>
                <div class="form-group">
                    <label>Description</label>
                    <textarea type="text" class="form-control" id="txtDepetmentDesc"></textarea>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="return AddDepartment();">Save</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="DegreeModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Add Degree</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>Degree</label>
                    <input type="text" class="form-control" id="txtDegree">
                </div>
                <div class="form-group">
                    <label>Description</label>
                    <textarea type="text" class="form-control" id="txtDegDesc"></textarea>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="return AddDegree();">Save</button>
            </div>
        </div>
    </div>
</div>






